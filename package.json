{
  "name": "neutron-im-web",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@matejmazur/react-katex": "^3.1.3",
    "@reduxjs/toolkit": "^1.5.1",
    "antd": "^4.15.5",
    "antd-img-crop": "^3.14.1",
    "bufferutil": "^4.0.3",
    "emoji-mart": "^3.0.1",
    "emoji-picker-react": "^3.4.4",
    "enquire-js": "^0.2.1",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "rc-queue-anim": "^1.8.5",
    "rc-scroll-anim": "^2.7.6",
    "rc-tween-one": "^2.7.3",
    "react": "^17.0.2",
    "react-contexify": "^5.0.0",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-icons": "^4.2.0",
    "react-intl": "^5.17.5",
    "react-markdown": "^6.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-syntax-highlighter": "^15.4.3",
    "react-virtualized": "^9.22.3",
    "react-virtualized-image-measurer": "^0.4.0",
    "redux": "^4.1.0",
    "remark-gfm": "^1.0.0",
    "remark-math": "^4.0.0",
    "umi-request": "^1.3.5",
    "utf-8-validate": "^5.0.5",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "react-app-rewired start --max_old_space_size=8192",
    "build": "react-app-rewired build --max_old_space_size=8192",
    "build:dev": "dotenv -e .env.development react-app-rewired build --max_old_space_size=8192",
    "test": "react-app-rewired --max_old_space_size=8192 test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.8",
    "@types/emoji-mart": "^3.0.4",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.168",
    "@types/mockjs": "^1.0.3",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/react-virtualized": "^9.21.11",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-import": "^1.13.3",
    "customize-cra": "^1.0.0",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "react-app-rewired": "^2.1.8",
    "typescript": "^4.2.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "import"
    ],
    "settings": {
      "webpack": "./config/webpack.config.js",
      "import/resolver": {
        "alias": {
          "@": "src"
        }
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "env": {
      "development": {
        "plugins": [
          "dynamic-import-node"
        ]
      }
    },
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "less"
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
